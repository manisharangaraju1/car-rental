<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.carrental">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".admin.AddCar" />
        <activity android:name=".admin.AddCarModel" >

        </activity>
        <activity android:name=".admin.AddStation" />
        <activity android:name=".CarOptionsFilter" android:parentActivityName=".view.NearbyStations"/>
        <activity android:name=".view.CarModels" android:parentActivityName=".CarOptionsFilter"/>
        <activity android:name=".userauth.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.NearbyStations" />
        <activity android:name=".userauth.SignUpActivity" />
        <activity android:name=".help.Help"
            android:parentActivityName=".view.NearbyStations">
        </activity>
    </application>

</manifest>